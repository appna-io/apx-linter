{
  "name": "eslint-plugin-apx-eslint",
  "version": "2.0.1",
  "description": "ESLint plugin for AppNaX with flexible configuration options",
  "main": "index.js",
  "bin": {
    "apx-lint": "./bin/apx-lint.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "test": "node test/run-tests.js",
    "lint": "node bin/apx-lint.js",
    "lint:fix": "node bin/apx-lint.js --fix",
    "upload": "npm version patch --no-git-tag-version && npm publish"
  },
  "keywords": [
    "eslint",
    "plugin",
    "apx",
    "AppNaX",
    "typescript",
    "react",
    "linter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/appna-io/apx-linter.git"
  },
  "license": "ISC",
  "files": [
    "index.js",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@eslint/js": "^9.37.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "chalk": "^4.1.2",
    "eslint": "^9.37.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
